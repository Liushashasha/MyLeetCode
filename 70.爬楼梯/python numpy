import numpy as np

class Solution(object):
    def climbStairs(self, n):
        """
        :type n: int
        :rtype: int
        """
        climb_array = np.zeros(n+1 ,dtype=int)
        climb_array[0]=climb_array[1]=1
        for i in range (2, n+1):
            climb_array[i]=climb_array[i-1]+climb_array[i-2]
        return climb_array[n]
